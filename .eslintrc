{
  "extends": [
    "react-app",
    "react-app/jest"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "warn",
    "quotes": ["warn", "single"],
    "jsx-quotes": ["warn", "prefer-single"],
    "prefer-const": "error",
    "indent": ["warn", 4],
    "comma-dangle": ["error", "always-multiline"],
    "semi": ["warn", "always"]
  }
}